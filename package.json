{
    "name": "@inseefr/lunatic-dsfr",
    "version": "1.0.21",
    "description": "Couche graphique pour Lunatic reposant sur le Système de Design de l'État (DSFR)",
    "repository": {
        "type": "git",
        "url": "git://github.com/inseefr/lunatic-dsfr.git"
    },
    "author": "INSEE (http://www.insee.fr)",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "knip": "knip",
        "build": "tsc",
        "check": "tsc --noEmit",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier \"**/*.{ts,tsx,json,md}\"",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "link-in-main-project": "ts-node --skipProject scripts/link-in-app.ts",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "prestorybook": "npx @inseefr/lunatic .storybook/static && copy-dsfr-to-public --publicDir .storybook/static && only-include-used-icons --publicDir .storybook/static",
        "prebuild-storybook": "yarn prestorybook",
        "prepare": "husky install"
    },
    "exports": {
        ".": "./dist/index.js",
        "./*": "./dist/*.js"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "files": [
        "src/",
        "dist/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://inseefr.github.io/Lunatic-DSFR",
    "peerDependencies": {
        "@emotion/react": "^11.4.1",
        "@mui/material": "^5.0.1"
    },
    "devDependencies": {
        "@codegouvfr/react-dsfr": "^1.7.2",
        "@emotion/react": "^11.10.6",
        "@emotion/styled": "^11.10.6",
        "@inseefr/lunatic": "2.7.19",
        "@mui/material": "^5.12.0",
        "@storybook/addon-a11y": "^7.6.17",
        "@storybook/addon-essentials": "^7.6.14",
        "@storybook/addon-interactions": "^7.6.14",
        "@storybook/addon-links": "^7.6.14",
        "@storybook/react": "^7.6.14",
        "@storybook/react-vite": "^7.6.14",
        "@storybook/theming": "^7.6.14",
        "@types/node": "^20.11.17",
        "@types/react": "^18.2.25",
        "@types/react-dom": "^18.2.10",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-storybook": "^0.6.15",
        "husky": "^8.0.0",
        "knip": "^5.0.1",
        "lint-staged": "^11.2.6",
        "prettier": "^2.8.8",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "storybook": "^7.6.14",
        "storybook-dark-mode": "^3.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.3",
        "vite": "^5.1.1",
        "vite-tsconfig-paths": "^4.3.1"
    },
    "dependencies": {
        "react-number-format": "^5.1.4",
        "tss-react": "^4.9.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "resolutions": {
        "jackspeak": "2.1.1"
    }
}
