    "compile:ts": "tsc --outDir ./lib --module commonjs ./src/index.ts",
    "postinstall": "copy-dsfr-to-public",
    "prestart": "only-include-used-icons",
    "prebuild": "only-include-used-icons",
    "storybook": "start-storybook -p 6006",
