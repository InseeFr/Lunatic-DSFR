{
    "name": "@inseefr/lunatic-dsfr",
    "version": "1.0.14",
    "description": "Couche graphique pour Lunatic reposant sur le Système de Design de l'État (DSFR)",
    "repository": {
        "type": "git",
        "url": "git://github.com/Inseefr/lunatic-dsfr.git"
    },
    "author": "INSEE (http://www.insee.fr)",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "echo DODO",
        "build": "tsc",
        "check": "tsc --noEmit",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier \"**/*.{ts,tsx,json,md}\"",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "link-in-main-project": "ts-node --skipProject src/scripts/link-in-main-project.ts",
        "storybook": "copy-dsfr-to-public && yarn build && yarn only-include-used-icons && storybook dev -p 6006",
        "build-storybook": "yarn build && yarn only-include-used-icons && storybook build",
        "prestart": "only-include-used-icons",
        "prebuild": "only-include-used-icons"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "files": [
        "src/",
        "!src/scripts/",
        "dist/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "peerDependencies": {
        "@emotion/react": "^11.10.5"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.22.20",
        "@babel/preset-react": "^7.22.15",
        "@babel/preset-typescript": "^7.23.0",
        "@codegouvfr/react-dsfr": "0.76.2",
        "@emotion/react": "^11.10.6",
        "@storybook/addon-actions": "^7.4.6",
        "@storybook/addon-essentials": "^7.4.6",
        "@storybook/addon-interactions": "^7.4.6",
        "@storybook/addon-links": "^7.4.6",
        "@storybook/addon-mdx-gfm": "7.4.6",
        "@storybook/react": "^7.4.6",
        "@storybook/react-webpack5": "7.4.6",
        "@storybook/testing-library": "^0.2.2",
        "@types/node": "^16.4.9",
        "@types/react": "^18.2.25",
        "@types/react-dom": "^18.2.10",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "babel-loader": "^8.3.0",
        "chromatic": "^7.2.3",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-storybook": "^0.6.14",
        "husky": "^4.3.8",
        "lint-staged": "^11.1.1",
        "prettier": "^2.3.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "storybook": "next",
        "ts-node": "^10.9.1",
        "tsconfig-paths-webpack-plugin": "^4.0.0",
        "typescript": "5.2.2"
    },
    "dependencies": {
        "@emotion/styled": "^11.10.6",
        "@inseefr/lunatic": "2.6.5",
        "@mui/material": "^5.12.0",
        "classnames": "^2.3.2",
        "react-number-format": "^5.1.4",
        "sass-loader": "^13.2.0",
        "tss-react": "4.8.6"
    },
    "resolutions": {
        "jackspeak": "2.1.1"
    },
    "jest": {
        "transformIgnorePatterns": [
            "node_modules/(?!@codegouvfr/react-dsfr)"
        ]
    }
}
